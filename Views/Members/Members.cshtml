@model List<Members>

@{
    ViewData["Title"] = "Members List";
}
<h1>Members List</h1>
<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Phone No</th>
        <th>Email Id </th>
    </tr>
    @foreach (var member in Model)
    {
        <tr>
            <td><code>@member.FirstName</code> </td>
            <td><code>@member.LastName</code> </td>
            <td><code>@member.PhoneNo</code> </td>
            <td><code>@member.EmailId</code> </td>
        </tr>
    }
</table>
